  58              	/*
  59              	** This routine saves the machine state, calls the ISR, and then
  60              	** restores the machine state and returns from the interrupt.
  61              	**
  62              	********************************************************************
  63              	********************************************************************
  64              	** NOTE:  this code is highly application-specific, and will most **
  65              	** probably require modification to tailor it.                    **
  66              	**                                                                **
  67              	** Examples of mods:  switch to/from user stack, context switch   **
  68              	** changes, etc.                                                  **
  69              	********************************************************************
  70              	********************************************************************
  71              	*/
  72              	
  73              	isr_save:
  74              	
  75              	/*
  76              	** Begin by saving the CPU state (except for the FP context information).
  77              	**
  78              	** At this point, the stack looks like this:
  79              	**
  80              	**  esp ->  vector #		saved by the entry macro
  81              	**	    error code, or 0	saved by the hardware, or the entry macro
  82              	**	    saved EIP		saved by the hardware
  83              	**	    saved CS		saved by the hardware
  84              	**	    saved EFLAGS	saved by the hardware
  85              	*/
  86 0000 60       		pusha			// save E*X, ESP, EBP, ESI, EDI
  87 0001 1E       		pushl	%ds		// save segment registers
  88 0002 06       		pushl	%es
  89 0003 0FA0     		pushl	%fs
  90 0005 0FA8     		pushl	%gs
  91 0007 16       		pushl	%ss
  92              	
  93              	/*
  94              	** Stack contents (all 32-bit longwords) and offsets from ESP:
  95              	**
  96              	**   SS GS FS ES DS EDI ESI EBP ESP EBX EDX ECX EAX vec cod EIP CS EFL
  97              	**   0  4  8  12 16 20  24  28  32  36  40  44  48  52  56  60  64 68
  98              	**
  99              	** Note that the saved ESP is the contents before the PUSHA.
 100              	**
 101              	** Set up parameters for the ISR call.
 102              	*/
 103 0008 8B442434 		movl	52(%esp),%eax	// get vector number and error code
 104 000c 8B5C2438 		movl	56(%esp),%ebx
 105              	/*
 106              	** MOD for 20205
 107              	*/
 108              	
 109              	/*
 110              	** We need to switch to the system stack.  This requires that we
GAS LISTING isr_stubs.s 			page 3


 111              	** save the user context pointer into the current PCB, then load
 112              	** ESP with the initial system stack pointer.
 113              	**
 114              	** THIS IS INHERENTLY NON-REENTRANT.
 115              	*/
 116              	
 117              	        .globl  _current
 118              	        .globl  _system_esp
 119              	
 120              	        // save the context pointer
 121              	        // (ASSUMES it is the first field in the PCB!)
 122 0010 8B150000 	        movl    _current, %edx
 122      0000
 123 0016 8922     	        movl    %esp, (%edx)
 124              	
 125              	        // switch to the system stack
 126 0018 8B250000 	        movl    _system_esp, %esp
 126      0000
 127              	
 128              	/*
 129              	** END MOD for 20205
 130              	*/
 131 001e 53       		pushl	%ebx		// put them on the top of the stack ...
 132 001f 50       		pushl	%eax		// ... as parameters for the ISR
 133              	
 134              	/*
 135              	** Call the ISR
 136              	*/
 137 0020 8B1C8500 		movl	__isr_table(,%eax,4),%ebx
 137      000000
 138 0027 FFD3     		call	*%ebx
 139 0029 83C408   		addl	$8,%esp		// pop the two parameters
 140              	
 141              	/*
 142              	** Context restore begins here
 143              	*/
