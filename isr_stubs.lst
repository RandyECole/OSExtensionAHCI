  31              	/*
  32              	** Macros for the isr stubs.  Some interrupts push an error code on
  33              	** the stack and others don't; for those that don't we simply push
  34              	** a zero so that cleaning up from either type is identical.
  35              	**
  36              	** Note: these are not marked as global symbols, as they are never
  37              	** accessed directly outside of this file.  This could be changed
  38              	** if need be by adding this line to each macro definition right
  39              	** after the #define line:
  40              	**
  41              	**	.global __isr_##vector
  42              	*/
  43              	
  44              	#define	ISR(vector)			\
  45              	__isr_##vector:				; \
  46              		pushl	$0			; \
  47              		pushl	$vector			; \
  48              		jmp	isr_save
  49              	
  50              	#define	ERR_ISR(vector)		\
  51              	__isr_##vector:				; \
  52              		pushl	$vector			; \
  53              		jmp	isr_save
